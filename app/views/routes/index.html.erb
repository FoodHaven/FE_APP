<% if @routes.present? %>
  <% @routes.each do |route| %>
    <p>
      Agency: <%= route.agency_name %> <br>
      Route: <%= route.route_short_name %> <br>
      Headsign: <%= route.headsign %> <br>
      Start Time: <%= Time.at(route.start_time).strftime("%B %e, at %I:%M %p") %> <br>
      End Time: <%= Time.at(route.end_time).strftime("%B %e, at %I:%M %p") %> <br>
    <p>
  <% end %>
<% else %>
  <h1>No routes found</h1>
<% end %>