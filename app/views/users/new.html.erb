<%= form_with url: users_path, method: :post do|f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>
  <%= f.label :password, "Password" %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation, "Confirm Password" %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Create Account" %>
<% end %>
